div(id="page-data")
  label(for="title")
    span(class="label-text") Title:
    div(id="page-title")
      input(
        name="title"
        type="text"
        class="input input-bordered w-full"
        value= pageTitle
      )
  br
  input(class="btn btn-primary" type="button" hx-delete="/admin/page" hx-include="[name='pageId']" value="Delete") 
  input(class="btn btn-primary" type="submit" value="Update")
